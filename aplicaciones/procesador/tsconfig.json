{
  "compilerOptions": {
    "module": "CommonJS",
    "target": "ESNext",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "moduleResolution": "node",
    "outDir": "./publico",
    "baseUrl": "./fuente",
    "paths": {
      "@/*": ["*"],
      "@/datosVisibles/*": ["../../datosVisibles/*"]
    }
  },
  "include": ["./fuente/**/*"],
  "exclude": ["node_modules"]
}
