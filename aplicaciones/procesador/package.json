{
  "name": "procesador",
  "version": "1.0.0",
  "description": "Procesador de datos para ¡Niñez Ya!",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": " nodemon ./fuente/aplicacion.ts",
    "pesos": "nodemon ./fuente/datosDescarga.ts"
  },
  "devDependencies": {
    "@types/cli-color": "^2.0.6",
    "@types/cli-progress": "^3.11.6",
    "@types/geojson": "^7946.0.14",
    "@types/node": "^22.5.0",
    "nodemon": "^3.1.4",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.3"
  },
  "dependencies": {
    "cli-color": "^2.0.4",
    "cli-progress": "^3.12.0",
    "node-emoji": "^2.1.3",
    "xlstream": "^2.5.5",
    "xlsx": "^0.18.5"
  },
  "nodemonConfig": {
    "ignore": [
      "./datos/*"
    ],
    "exec": "node -r tsconfig-paths/register -r ts-node/register ./fuente/aplicacion.ts && node -r tsconfig-paths/register -r ts-node/register ./fuente/datosDescarga.ts",
    "ext": "ts, js"
  }
}
